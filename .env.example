# Base URL for image resources (e.g., http://127.0.0.1:8000)
BASE_URL=

# Debug mode (set to True for development, False for production)
DJANGO_DEBUG=True

# Secret key for your Django project (set a unique, random value)
DJANGO_SECRET_KEY=

# Allowed hosts for your Django project (comma-separated values)
# Example: DJANGO_ALLOWED_HOSTS=localhost,127.0.0.1
DJANGO_ALLOWED_HOSTS=

# Enable Django Debug Toolbar (set to True to enable)
DEBUG_TOOLBAR_ENABLED=True

# Show Debug Toolbar callback (set to True to show the toolbar)
SHOW_TOOLBAR_CALLBACK=True

# PostgreSQL Database Configuration
# Name of the database
DB_NAME=
# Username for the database
DB_USER=
# Password for the database user
DB_PASSWORD=
# Host where the database server is running
DB_HOST=
# Port number for the database server
DB_PORT=

# CORS origin whitelist (comma-separated values)
# Example: CORS_ORIGIN_WHITELIST=http://localhost:3000,http://127.0.0.1:3000
CORS_ORIGIN_WHITELIST=

# CORS related settings
# Base URL for the backend
DJANGO_BASE_BACKEND_URL=

# Base URL for the frontend
DJANGO_BASE_FRONTEND_URL=

# Whitelisted CORS origins for the project
# Example: DJANGO_CORS_ORIGIN_WHITELIST=http://localhost:3000,http://127.0.0.1:3000
DJANGO_CORS_ORIGIN_WHITELIST=

# Email Configuration
# Host for the email server
EMAIL_HOST=
# Port for the email server
